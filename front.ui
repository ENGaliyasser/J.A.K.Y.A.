       <string notr="true">/* Main Window */
QMainWindow {
    background-color: #f0f4f8;
    font-family: Arial, Helvetica, sans-serif;
}

/* Tab Widget */
QTabWidget::pane {
    border: 1px solid #cccccc;
    background-color: #ffffff;
}

QTabBar::tab {
    background: #dce4ec;
    color: #004c8c;
    border: 1px solid #aaaaaa;
    padding: 10px;
    min-width: 120px;
    font-weight: bold;
}

QTabBar::tab:selected {
    background: #004c8c;
    color: #ffffff;
}

/* Labels */
QLabel {
    color: #004c8c;
    font-weight: bold;
}

/* Push Buttons */
QPushButton {
    background-color: #008cba;
    color: white;
    border-radius: 10px;
    padding: 8px;
    font-size: 14px;
}

QPushButton:hover {
    background-color: #005f7a;
}

QPushButton:pressed {
    background-color: #003f50;
}

/* Group Boxes */
QGroupBox {
    border: 2px solid #004c8c;
    border-radius: 5px;
    margin-top: 20px;
    color: #004c8c;
    font-weight: bold;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 5px;
}

/* Text Edit */
QTextEdit {
    background-color: #ffffff;
    color: #004c8c;
    border: 1px solid #cccccc;
    padding: 8px;
}

/* Line Edit */
QLineEdit {
    background-color: #ffffff;
    color: #004c8c;
    border: 1px solid #cccccc;
    padding: 5px;
    border-radius: 5px;
}

/* Combo Box */
QComboBox {
    background-color: #ffffff;
    color: #004c8c;
    border: 1px solid #cccccc;
    padding: 5px;
    border-radius: 5px;
}

QComboBox:hover {
    border: 1px solid #008cba;
}

/* Status Bar */
QStatusBar {
    background-color: #004c8c;
    color: #ffffff;
}

/* Scroll Bar */
QScrollBar:vertical {
    background-color: #dce4ec;
    width: 12px;
    margin: 20px 0 20px 0;
    border: 1px solid #aaaaaa;
}

QScrollBar::handle:vertical {
    background-color: #004c8c;
    min-height: 20px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    background-color: #dce4ec;
    border: 1px solid #aaaaaa;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

/* Tool Bar */
QToolBar {
    background-color: #004c8c;
    border-bottom: 1px solid #cccccc;
}

QToolButton {
    background-color: transparent;
    border: none;
    color: white;
    padding: 5px;
}

QToolButton:hover {
    background-color: #005f7a;
}</string>
